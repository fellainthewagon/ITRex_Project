{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": { "prettier/prettier": "error", "no-console": 1 }
}
